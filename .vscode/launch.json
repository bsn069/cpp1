{
	"version": "0.2.0",
	"configurations": [

		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"miDebuggerPath": "c:/MinGW/bin/gdb.exe",
			"program": "${workspaceRoot}/test/main.exe",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		},
		{
			"type": "gdb",
			"request": "launch",
			"name": "(Windows) gdb",
			"target": "${workspaceRoot}/test/main.exe",
			"cwd": "${workspaceRoot}"
		},
		{
			"name": "(Windows) Launch",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceRoot}/test/main.exe",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}",
			"environment": [],
			"externalConsole": true
		}
	]
}